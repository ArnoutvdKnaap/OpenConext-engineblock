<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                                       http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">
    <changeSet author="boy@ibuildings.nl" id="1308826012-1">
        <createTable tableName="group_provider_user_oauth">
            <column name="provider_id" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="user_id" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="oauth_token" type="VARCHAR(1024)">
                <constraints nullable="false"/>
            </column>
            <column name="oauth_secret" type="VARCHAR(1024)">
                <constraints nullable="false"/>
            </column>
        </createTable>
        <addPrimaryKey columnNames="provider_id, user_id" tableName="group_provider_user_oauth"/>
    </changeSet>
</databaseChangeLog>